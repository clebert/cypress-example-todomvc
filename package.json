{
  "name": "cypress-test",
  "private": true,
  "scripts": {
    "commitmsg": "commitlint --edit",
    "format": "prettier --write '**/*.{js,json,md,ts}'",
    "start": "cypress open",
    "test": "run-s test:*",
    "test:prettier": "prettier --list-different '**/*.{js,json,md,ts}'",
    "test:tslint": "tslint --config tslint.json --project cypress/tsconfig.json --formatters-dir node_modules/custom-tslint-formatters/formatters --format grouped"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-angular": "^6.1.3",
    "@cypress/webpack-preprocessor": "^2.0.1",
    "custom-tslint-formatters": "^2.3.0",
    "cypress": "^2.1.0",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "ts-config": "^18.0.0",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "webpack": "^4.9.1"
  }
}
